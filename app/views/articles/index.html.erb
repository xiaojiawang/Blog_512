<% content_for :content do %><br>
<%= link_to '新建文章', new_article_path %>
<table class="table table-striped">
<tr>
<th>标题</th>
<th>作者</th>
<th>创建时间</th>
<th>操作</th>
</tr>
<% @articles.each do |f| %>
<tr>
<td><%= f.title %></td>
<td><%= f.author %></td>
<td><%= f.created_at%></td>
<!-- <td><%#= f.created_at.strftime("%Y-%m-%d %H:%M:%S") %></td> -->
<td><%= link_to '显示', f %></td>
</tr>

<% end %>
</table>

<% end %>

